CASE 

WHEN NBR_OFICINA ="BEC SUR CHICO" THEN 20
WHEN NBR_OFICINA ="BE PIURA" THEN 19
WHEN NBR_OFICINA ="BEC LIMA ORIENTE" THEN 18
WHEN NBR_OFICINA ="BE AREQUIPA" THEN 17
WHEN NBR_OFICINA ="BE LA MOLINA" THEN 16
WHEN NBR_OFICINA ="BE DOS DE MAYO" THEN 15
WHEN NBR_OFICINA ="BE HUANCAYO" THEN 14
WHEN NBR_OFICINA ="BE CHICLAYO" THEN 13
WHEN NBR_OFICINA ="BE REPUBLICA DE PANAMA" THEN 12
WHEN NBR_OFICINA ="BE LIMA OESTE 2" THEN 11
WHEN NBR_OFICINA ="BEC LIMA 1" THEN 10
WHEN NBR_OFICINA ="BEC NORTE CHICO" THEN 9
WHEN NBR_OFICINA ="BE CHACARILLA" THEN 8
WHEN NBR_OFICINA ="BE MIRAFLORES" THEN 7
WHEN NBR_OFICINA ="BE LIMA OESTE 1" THEN 6
WHEN NBR_OFICINA ="BE SAN ISIDRO" THEN 5
WHEN NBR_OFICINA ="BE CUSCO" THEN 4
WHEN NBR_OFICINA ="BE TRUJILLO" THEN 3
WHEN NBR_OFICINA ="BEC LIMA CENTRO" THEN 2
WHEN NBR_OFICINA ="BE SANTA CRUZ" THEN 1

END